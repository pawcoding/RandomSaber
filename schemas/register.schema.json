{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://randomsaber.pawcode.de/schemas/register.schema.json",
  "title": "Music-Pack Register",
  "description": "Register of all currently supported music-packs",
  "type": "object",
  "properties": {
	"packs": {
	  "description": "List of all music-packs",
	  "type": "array",
	  "minItems": 1,
	  "uniqueItems": true,
	  "items": {
		"description": "Filename for music-pack",
		"type": "string"
	  }
	}
  },
  "required": ["packs"]
}