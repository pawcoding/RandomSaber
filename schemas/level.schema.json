{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://randomsaber.pawcode.de/schemas/level.schema.json",
  "title": "Level",
  "description": "Level in a music-pack",
  "type": "object",
  "properties": {
	"song": {
	  "description": "Name of the song / level",
	  "type": "string"
	},
	"modes": {
	  "description": "List in which modes / difficulties the level can be played",
	  "$ref": "https://randomsaber.pawcode.de/schemas/modes.schema.json"
	}
  },
  "required": ["song", "modes"]
}